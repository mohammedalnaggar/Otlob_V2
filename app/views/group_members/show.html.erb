<div class="page-header">
  <%= link_to group_members_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    All Group_members
  <% end %>
  <%= link_to edit_group_member_path(@group_member), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show group_member</h1>
</div>

<dl class="dl-horizontal">
  <dt>User:</dt>
  <dd><%= @group_member.user %></dd>

  <dt>Group:</dt>
  <dd><%= @group_member.group %></dd>

</dl>
